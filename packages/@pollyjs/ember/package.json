    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "proxy-server-middleware"
    ],
    "after": [
      "ember-auto-import"
    ]
