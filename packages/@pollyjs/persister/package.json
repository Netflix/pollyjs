      "email": "offirgolan@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@pollyjs/utils": "^6.0.1",
    "@types/set-cookie-parser": "^2.4.1",
    "bowser": "^2.4.0",
    "fast-json-stable-stringify": "^2.1.0",
    "lodash-es": "^4.17.21",
    "set-cookie-parser": "^2.4.8",
    "utf8-byte-length": "^1.0.4"
  },
  "devDependencies": {
    "har-validator": "^5.1.5",
    "rollup": "^1.14.6"
  }
