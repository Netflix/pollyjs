{
  "name": "@pollyjs/persister-fs",
  "version": "1.0.1",
  "description": "File system persister for @pollyjs",
  "main": "dist/cjs/pollyjs-persister-fs.js",
  "module": "dist/es/pollyjs-persister-fs.js",
  "files": [
    "src",
    "dist"
  ],
